{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "cdm",
      "camelcase": "cdm",
      "title": "CDM",
      "scope": "source.cdm",
      "file-types": [
        "cdm"
      ],
      "injection-regex": "^cdm$",
      "class-name": "TreeSitterCdm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Contextual Data Modeling Language",
    "authors": [
      {
        "name": "Aaron Larner",
        "email": "aaron@larner.dev",
        "url": "https://www.larner.dev/"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-cdm"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}